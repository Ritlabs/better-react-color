{
  "name": "@ritlabs/better-react-color",
  "version": "2.19.3",
  "description": "A Collection of Color Pickers from Sketch, Photoshop, Chrome & more",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ritlabs/better-react-color.git"
  },
  "author": "case <case@casesandberg.com> (original repo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ritlabs/better-react-color/issues"
  },
  "scripts": {
    "test": "npm run jest && npm run eslint",
    "jest": "jest",
    "eslint": "eslint src/**/*.js",
    "lib": "npm run clean-lib && babel src -d lib",
    "es": "npm run clean-es && babel src -d es",
    "clean-lib": "rm -rf lib && mkdir lib",
    "clean-es": "rm -rf es && mkdir es",
    "prepublish": "npm run lib && npm run es",
    "docs": "npm run docs-server",
    "docs-server": "node ./scripts/docs-server",
    "docs-dist": "node ./scripts/docs-dist",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "chromatic": "chromatic --project-token=9z2kaxsuz"
  },
  "sideEffects": false,
  "homepage": "http://ritlabs.com/",
  "keywords": [
    "react",
    "color picker",
    "react-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ],
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@icons/material": "^0.2.4",
    "@types/react": "^18.0.15",
    "@types/tinycolor2": "^1.4.3",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "material-colors": "^1.2.1",
    "prop-types": "^15.5.10",
    "reactcss": "^1.2.0",
    "tinycolor2": "^1.4.1",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@case/eslint-config": "^0.3.1",
    "@storybook/addon-centered": "^5.3.21",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/react": "^6.5.9",
    "chai": "^3.3.0",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.24.0",
    "enzyme": "2.8.2",
    "event-stream": "^3.3.1",
    "fbjs": "^0.8.6",
    "highlight.js": "^9.3.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^3.2.0",
    "i": "^0.3.5",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.4.5",
    "normalize.css": "^4.1.1",
    "npm": "^5.3.0",
    "object-assign": "^4.1.0",
    "react": "^18.2.0",
    "react-addons-test-utils": "^0.14.0 || ^15.0.0",
    "react-context": "0.0.3",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^1.2.8",
    "react-mark": "0.0.4",
    "react-test-renderer": "^15.3.2",
    "remarkable": "^1.6.0",
    "require-dir": "^0.3.0",
    "rimraf": "^2.5.0",
    "source-map-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "testdom": "^2.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-dev-server": "^4.9.3"
  },
  "files": [
    "lib",
    "es"
  ],
  "jest": {
    "rootDir": "src",
    "testRegex": "spec.js$"
  },
  "eslintConfig": {
    "extends": "@case",
    "rules": {
      "no-magic-numbers": 0
    }
  },
  "payload": {
    "builds": [
      {
        "cmd": "npm run docs-dist",
        "files": [
          "docs/build/bundle.js",
          "docs/index.js"
        ]
      }
    ]
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib"
  }
}
